<!-- Adicionando produtos à lista -->
<script>
function AddProduto(codigo){

	$.ajax({
	url: 'http://www.pagpouco.com/action/atualizarlista.php',
	type: 'POST',
	data:({
	codigo: codigo
	}),

	success:function(result) {

		var resultado = jQuery.parseJSON(result);

		if(resultado.botao == "N"){

			document.getElementById(codigo + "N").style.display = 'inline';
			document.getElementById(codigo + "S").style.display = 'none';

		} else {

			document.getElementById(codigo + "N").style.display = 'none';
			document.getElementById(codigo + "S").style.display = 'inline';

		}

		$("#lista_qtd1").html(resultado.qtd);
		$("#lista_qtd2").html(resultado.qtd);

	}
});
}

function AddProduto2(codigo){

	$.ajax({
	url: 'http://www.pagpouco.com/action/atualizarlista.php',
	type: 'POST',
	data:({
	codigo: codigo
	}),

	success:function(result) {

		var resultado = jQuery.parseJSON(result);

		if(resultado.botao == "N"){

			document.getElementById(codigo + "N2").style.display = 'block';
			document.getElementById(codigo + "S2").style.display = 'none';

		} else {

			document.getElementById(codigo + "N2").style.display = 'none';
			document.getElementById(codigo + "S2").style.display = 'block';

		}

		$("#lista_qtd1").html(resultado.qtd);
		$("#lista_qtd2").html(resultado.qtd);

	}
});
}
</script>

